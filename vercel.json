{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/*.(svg|png)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=604800, immutable"
        }
      ]
    }
  ]
}
